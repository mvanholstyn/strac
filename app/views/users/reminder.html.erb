<div class="notice"><%= flash[:notice] %></div>
<div class="error"><%= flash[:error] %></div>

<% form_for :user do |form| -%>
	<p>
  	<b>Email Address</b><br />
		<%= form.text_field :email_address %>
  </p>

	<p>
  	<%= submit_tag 'Submit' %>
	</p>
<% end -%>

<%= link_to "Login", :action => "login" %>