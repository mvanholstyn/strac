<div class="section">
  <div class="links">
    <%= link_to 'New Project', new_project_path, :class => "action" %>
  </div>
<h2>Projects</h2>

<table>
<% for project in @projects %>
  <tr>
    <td><%=h project.name %></td>
    <td><%= link_to 'Show', project_path(project) %></td>
    <td><%= link_to 'Edit', edit_project_path(project) %></td>
    <td><%= link_to 'Destroy', project_path(project), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

